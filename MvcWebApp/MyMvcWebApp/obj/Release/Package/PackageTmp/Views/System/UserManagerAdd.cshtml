@{
    Layout = null;

}
<form id="f_edit">
    <table cellpadding="0" cellspacing="0" style="width: 100%; text-align: left; table-layout: fixed; border: 1px solid #BABABA; padding: 8px 4px 4px 4px;margin-bottom:4px ">
        <tr style="padding: 4px 4px 4px 4px;">
            <td class="myfirsttdnm">用戶編號:</td>
@if (!this.Request["EORA"].Equals("a")) {
            <td class="myfirsttdnm"><input type="text" name="USER_ID" value="@this.Request["KEYS"]" class="easyui-validatebox" style="width:140px;background-color:#b0aeae;color:#888" readonly="readonly" /></td>
}
else
{<td class="myfirsttdnm"><input type="text" id="txt_userid" name="USER_ID" value="" class="easyui-validatebox" maxlength="10" required="true" style="width:140px" /><font color="red" style="margin-left:2px">*</font></td> }
            <td class="myalldr">角色:</td>
            <td class="myalldr"><input type="text" name="ROLE_ID" value="@(this.Request["EORA"].Equals("a") ? "1" : this.Request["ROLE_ID"].ToString())" data-options="valueField: 'id',textField: 'text',url: '/System/UserManagerCombRoleID'" class="easyui-combobox" required style="width:140px" /></td>
        </tr>
        <tr>
            <td class="myfirsttdnm">是否啟用:</td>
            <td class="myfirsttdnm"><input type="text" name="ENABLE_FLAG" value="@(this.Request["EORA"].Equals("a") ? "0" : this.Request["ENABLE_FLAG"].ToString())" data-options="valueField: 'id',textField: 'text',url: '/System/UserManagerCombEnableFlag'" class="easyui-combobox" required style="width:140px" /></td>
            <td class="myalldr"></td>
            <td class="myalldr"></td>

        </tr>
    </table>

    <input id="bt_valid" class="mybtn" type="button" value="校验" style="display:none" />
    <input id="v_s_action" type="text" style="display:none" value="@ViewBag.MySaveAction" />
    <input id="tx_save" type="text" style="display:none" />
    <input type="text" name="KEYS" style="display:none" value="@(this.Request["EORA"].Equals("a") ? "" : this.Request["KEYS"])" />
</form>

<script src="@Url.Content("~/Scripts/jquery.min.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
