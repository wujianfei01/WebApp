        <table cellpadding="0" cellspacing="0" style="width: 100%; text-align: left; table-layout: fixed; border: 1px solid #BABABA; padding: 8px 4px 4px 4px;margin-bottom:4px ">
            <tr>
                <td style="font-size: smaller; font-weight: bold; column-span: all; ">基础数据查询</td>
            </tr>
            <tr style="padding: 4px 4px 4px 4px;">
                <td class="myfirsttdnm">基础参数名:</td>
                <td class="myfirsttdnm"><input type="text" name="basename" class="easyui-validatebox" validtype="length[1,30]" style="width:120px" /></td>
            </tr>
        </table>
    </form>
    <div>
        <input id="v_q_action" type="hidden" value="@ViewBag.MyQueryAction" />
        <input id="v_e_action" type="hidden" value="@ViewBag.MyEditAction" />
        <input id="v_a_action" type="hidden" value="@ViewBag.MyAddAction" />
        <input id="v_d_action" type="hidden" value="@ViewBag.MyDeleteAction" />
        <input id="bt_search" class="mybtn" type="button" value="查询" />
        <input id="bt_add" class="mybtn" type="button" value="新增" />
        <input id="bt_del" class="mybtn" type="button" value="删除" />
        <input id="bt_clearall" class="mybtn" type="button" value="清除" />
    </div>
    <table id="datagrid" class="easyui-datagrid">
        <thead>
            <tr>
                <th checkbox="true"></th>
                <th field="KEYS">KEYS</th>
                <th field="OPT" width="20" align="center" data-options="formatter:OperateFormatter,styler:MyCellStyler">操作</th>
                <th field="BASE_NAME" align="center" width="60">基础参数名称</th>
                <th field="BASE_CODE" width="60">基础参数编码</th>
                <th field="BASE_TYPE" width="60">分组类别</th>
                <th field="CREATE_DATE" width="60">创建时间</th>
                <th field="LAST_EDIT_BY" width="60">最近编辑人</th>
                <th field="DESCRIPTION" width="60">描述</th>
                <th field="ENABLE" width="60" data-options="hidden:true"></th>
            </tr>
        </thead>
    </table>

    <div id="edit" class="easyui-dialog" closed="true" buttons="#dlg-buttons" title="添加或编辑基础数据" style="width:900px;height:300px;">
        <iframe id='editIframe' name='editIframe' frameborder="0" style="width:100%;height:100%;overflow:scroll;overflow-y:hidden;"></iframe>
    </div>
    <div id="dlg-buttons">
        <input id="bt_save" class="mybtn" type="button" value="保存" />
        <input id="bt_close" class="mybtn" type="button" value="取消" />
    </div>

