@{
    Layout = null;

}
<!DOCTYPE html>

<html>
<head>
    <title>System.UserManager</title>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
</head>
<body>
    <form id="f_search">
        <table cellpadding="0" cellspacing="0" style="width: 100%; text-align: left; table-layout: fixed; border: 1px solid #BABABA; padding: 8px 4px 4px 4px;margin-bottom:4px ">
            <tr>
                <td style="font-size: smaller; font-weight: bold; column-span: all; ">用户查询</td>
            </tr>
            <tr style="padding: 4px 4px 4px 4px;">
                <td class="myfirsttdnm">用户编号:</td>
                <td class="myfirsttdnm"><input type="text" name="userid" class="easyui-validatebox" validtype="length[1,30]" style="width:120px" /></td>
            </tr>
        </table>
    </form>
    <div>
        <input id="v_q_action" type="hidden" value="@ViewBag.MyQueryAction" />
        <input id="v_e_action" type="hidden" value="@ViewBag.MyEditAction" />
        <input id="v_a_action" type="hidden" value="@ViewBag.MyAddAction" />
        <input id="v_d_action" type="hidden" value="@ViewBag.MyDeleteAction" />
        <input id="bt_search" class="mybtn" type="button" value="查询" />
        <input id="bt_add" class="mybtn" type="button" value="新增" />
        <input id="bt_del" class="mybtn" type="button" value="删除" />
        <input id="bt_clearall" class="mybtn" type="button" value="清除" />
    </div>
    <table id="datagrid" class="easyui-datagrid">
        <thead>
            <tr>
                <th checkbox="true"></th>
                <th field="KEYS">KEYS</th>
                <th field="USER_ID" width="60">用户编号</th>
                <th field="ROLE_ID" width="60">角色编号</th>
                <th field="ROLE_DESCRIPTION" width="60">角色描述</th>
                <th field="ENABLES" width="60">是否启用</th>
                <th field="ENABLE_FLAG" width="60" data-options="hidden:true">启用</th>
                <th field="CREATION_DATE" width="60">创建日期</th>
                <th field="CREATED_BY" width="60" data-options="hidden:true">创建人</th>
                <th field="USER_NAME" width="60">用户名</th>
                <th field="E_USER_NAME" width="60">英文用户名</th>
                <th field="DEPT_NO" width="60">部门编号</th>
            </tr>
        </thead>
    </table>

    <div id="edit" class="easyui-dialog" closed="true" buttons="#dlg-buttons" title="添加或编辑用户" style="width:800px;height:300px;">
        <iframe id='editIframe' name='editIframe' frameborder="0" style="width: 100%; height: 100%; overflow: scroll; overflow-y: hidden;"></iframe>
    </div>
    <div id="dlg-buttons">
        <input id="bt_save" class="mybtn" type="button" value="保存" />
        <input id="bt_close" class="mybtn" type="button" value="取消" />
    </div>

    <script src="@Url.Content("~/Scripts/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.easyui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/easyui/locale/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jBox/jquery.jBox-2.3.min.js")"></script>
    <script src="@Url.Content("~/Scripts/control/pagecontrol.js")"></script>
    <script src="@Url.Content("~/Scripts/control/querypage.js")"></script>
    <script src="@Url.Content("~/Scripts/control/foreditpage.js")"></script>
    <link href="@Url.Content("~/Scripts/jBox/Skins/GrayCool/jbox.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/icon.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/gray/easyui.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/controls/MyStyle.css")" rel="stylesheet" />
</body>
</html>